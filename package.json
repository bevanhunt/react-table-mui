{
  "name": "react-table-mui-ts",
  "version": "0.0.20",
  "private": true,
  "type": "module",
  "source": "index.ts",
  "module": "./dist/react-table-mui-ts.modern.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "mb": "microbundle --globals react=React --jsx React.createElement --jsxFragment React.Fragment",
    "build": "rimraf ./dist && yarn run mb --no-compress && npm-dts -o ./dist/index.d.ts generate",
    "start": "yarn run mb watch --no-compress"
  },
  "engines": {
    "node": "^16.15.0"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.2",
    "@mui/material": "^5.8.2",
    "@mui/styles": "^5.8.0",
    "@mui/system": "^5.8.2",
    "classnames": "^2.3.1",
    "match-sorter": "^6.3.1",
    "prop-types": "^15.8.1",
    "react-json-view": "^1.21.3",
    "react-table": "^7.7.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-table": "^7.7.12",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "8.16.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "microbundle": "^0.15.0",
    "npm-dts": "^1.3.11",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.2"
  }
}
